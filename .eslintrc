{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],

  "rules": {
    "prettier/prettier": ["error"],
    "camelcase": "off",
    "no-console": 2,
    "max-nested-callbacks": [2, 3],
    "no-duplicate-imports": 2,
    "no-undef": 0,
    "no-unused-vars": 0,
    "prefer-const": 2,
    "no-multiple-empty-lines": [2, { "max": 1, "maxEOF": 1 }],
    "no-multi-spaces": 2
  }
}
